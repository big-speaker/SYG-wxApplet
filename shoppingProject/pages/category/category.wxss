page{
    height:100%;
}
.categoryPage{
    height:100%;
}
.categoryPage .allGoods{
    height:calc(100vh - 90rpx);
    display:flex;
    flex-direction:row;
}
.leftMenus{
  flex:1;
  text-align:center;
}
.rightGood{
  flex:3;
} 
.leftMenus view{
  width:100%;
  height:60rpx;
}
.goodTitle{
  text-align:center;
  margin-bottom:20rpx;
  margin-top:10rpx;
}
.goodContent{
  width:100%;
  display:flex;
  flex-wrap:wrap;
}
image{
  width:100%;
}
.active{
  color:#007FFF;
  border-left:solid;
}
.goodContent navigator{
  margin:5rpx;
  display:flex;
  flex-direction:column;
  width:30%;
}